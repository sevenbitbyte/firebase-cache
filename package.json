  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "compression-webpack-plugin": "^0.3.1",
    "joi": "9.0.4",
    "moment": "2.x.x",
    "ngzip": "^1.1.3",
